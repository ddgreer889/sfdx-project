<aura:component implements="flexipage:availableForAllPageTypes" access="global"  controller="afNewBatchAndAvailabilityApexController">
    
    <aura:attribute name="allLocations"     type="List" access="public"/>
    <aura:attribute name="allRooms"     type="List" access="public"/>
    <aura:attribute name="allSkills"     type="List" access="public"/>
    <aura:attribute name="allTrainers"     type="List" access="public"/>
    <aura:attribute name="allTrainings"     type="List" access="public"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    
    <ltng:require styles="{!$Resource.RevatureStyles}"/>
    <div class="slds-card__body ">
        <div class="slds-grid slds-gutters">
            <div class="slds-col slds-size_7-of-12">
                <span>
                  <c:afNewBatchForm/>
                </span>
            </div>
            <div class="slds-col slds-size_5-of-12">
                <span>
                    <c:afAvailabilityListContainer allSkills="{!v.allSkills}"												   
												   allTrainings="{v.allTrainings}"
                                                   allRooms="{!v.allRooms}"
												   allTrainers="{!v.allTrainers}"/>
                </span>
            </div>
        </div>
    </div>     	
</aura:component>